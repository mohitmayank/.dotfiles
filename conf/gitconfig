  name = Mohit
[credential]
  helper = cache --timeout 2880000
[pull]
  ff = true
[push]
  default = current
[core]
  excludesfile = ~/.gitignore
  fileMode = true
  filemode = false
  filemode = true
[diff]
  tool = vimdiff
[merge]
  tool = meld
  conflictstyle = diff3
[mergetool]
  prompt = true
[credential]
  helper = cache --timeout=36000
[difftool]
  prompt = true
[alias]
  a = add
  d = difftool
  dm = "difftool --diff-filter=M"
  nfm = "!f(){ git -c core.fileMode=false $@; };f"
  s = status
  m = merge
  b = branch
  c = commit
  p = push
  f = pull
  co = checkout
  mt = mergetool
  A = "add -A"
  cm = "commit -m"
  gl = "log --graph --oneline --decorate --all"
  create-pull-request = !sh -c 'stash pull-request $0 $@'

[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[init]
	defaultBranch = master
